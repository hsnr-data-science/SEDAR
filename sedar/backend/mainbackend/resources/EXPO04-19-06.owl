    <rdfs:subClassOf rdf:resource="http://www.hozo.jp/owl/EXPOApr19.xml/PairedComparison"/>
    <owl:disjointWith rdf:resource="http://www.hozo.jp/owl/EXPOApr19.xml/PairedComparisonOfMatchingGroups"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >PairedComparisonOfSingleSampleGroups</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.hozo.jp/owl/EXPOApr19.xml/User">
