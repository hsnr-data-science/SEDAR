  <owl:Class rdf:about="http://www.hozo.jp/owl/EXPOApr19.xml/MortalityEffect">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.hozo.jp/owl/EXPOApr19.xml/ObjectEffect"/>
