:InvestigateGoal rdf:type owl:Class ;
                 rdfs:subClassOf :ExperimentalGoal ;
                 rdfs:comment """
An investigate-goal is an experimental goal of a Baconian experiment.
"""^^xsd:string ;
                 rdfs:label "InvestigateGoal"^^xsd:string .


###  http://www.hozo.jp/owl/EXPOApr19.xml/Journal
:Journal rdf:type owl:Class ;
         rdfs:subClassOf :Text ;
         rdfs:comment """
Dublin Core: http://www.lub.lu.se/cgi-bin/nmdc.pl
"""^^xsd:string ;
         rdfs:label "Journal"^^xsd:string .


